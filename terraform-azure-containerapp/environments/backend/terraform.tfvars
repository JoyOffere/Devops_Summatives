location = "East US"
backend_image = "acrcontainerappstaging.azurecr.io/scholardorm-backend:v3"
registry_server = "acrcontainerappstaging.azurecr.io"
registry_username = "acrcontainerappstaging"
registry_password = "YOUR_ACR_PASSWORD"  # Replace with your ACR admin password
environment_variables = {
  "NODE_ENV"  = "staging"
  "PORT"      = "5000"
  "MONGO_URI" = "YOUR_MONGODB_CONNECTION_STRING"  # Replace with your MongoDB connection string
}
cpu = "0.25"
memory = "0.5Gi"
